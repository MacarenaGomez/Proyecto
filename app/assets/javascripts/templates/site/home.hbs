<div class="span5"> 
  {{#each principal}}
  <div class="row expert">
    <div class="thumbnail" data-expert="{{this.screen_name}}">
      <img src="{{this.profile_image_url}}" style="float: left;margin: 5px;">
      <h3>{{this.expert}}</h3>
      <h4>{{this.location}}</h4>
      <br>
      <p>{{this.description}}</p>
    </div>
  </div>
  {{/each}}
</div>

<div class="span5"> 
  {{#each experts}}
    <div class="row expert">
      <div class="thumbnail" data-expert="{{this.screen_name}}">
        <img src="{{this.profile_image_url}}" style="float: left;margin: 5px;">
        <h3>{{this.expert}}</h3>
        <h4>{{this.location}}</h4>
        <br>
        <p>{{this.description}}</p>
      </div>
    </div>
{{/each}}
  </div>

      